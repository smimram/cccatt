#-# mode: category
#
# Plain categories
#

coh id {a : .} : a -> a

coh co {a b c : .} (f : a -> b) (g : b -> c) : a -> c

coh unit-l {a b : .} (f : a -> b) : co id f = f

coh unit-r {a b : .} (f : a -> b) : co f id = f

coh assoc {a b c d : .} (f : a -> b) (g : b -> c) (h : c -> d) : co (co f g) h = co f (co g h)