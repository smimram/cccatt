## Sanity checks

# Not habited
ncoh (a : *) : a

# Not habited
ncoh (a b : *) : a → b

# Not habited and non-terminating
ncoh (a : *) : (a → a) → a

# Not contractible
ncoh (a : *) : a → a → a

# Not contractible
ncoh (a : *) : (a → a) → a → a

# Contractible but not a coherence
ncoh (a b : *) : (b → a) → a → a

# Deterministic but not a coherence
ncoh (a b : *) : b → b → a → a

## Useful stuff

coh I (a : *) : a → a

coh K (a b : *) : a → b → a

coh S (a b c : *) : (a → b → c) → (a → b) → a → c

coh ap (a b : *) (t : a → b) (u : a) : b

coh comp (a b c : *) (f : a → b) (g : b → c) : a → c

## Testing variables

let id := I

let id (a : *) (x : a) := ap a a (I a) x

